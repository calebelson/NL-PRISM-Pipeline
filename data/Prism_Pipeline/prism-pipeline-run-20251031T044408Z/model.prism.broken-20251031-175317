// ASSUMPTION: node capacity unspecified => set per-node max to total initial resources = 8 // total resources from resources[] = 8
// ASSUMPTION: on a failed traversal the transported resources are lost and the team moves to 'fail' (-1) as in template
// ASSUMPTION: transport success probability = safety_probs entry; no team-specific factors (factor_team = 1.0)
// JSON:/graph/nodes -> a,b,c,d,e,f,g
// JSON:/teams[0] -> T1, teams[1] -> T2
// JSON:/resources -> initial quantities mapped below
// JSON:/demands -> target at node g qty 7

mdp

const int fail = -1;

// JSON:/graph/nodes
const int a = 0; // graph.nodes[0]
const int b = 1; // graph.nodes[1]
const int c = 2; // graph.nodes[2]
const int d = 3; // graph.nodes[3]
const int e = 4; // graph.nodes[4]
const int f = 5; // graph.nodes[5]
const int g = 6; // graph.nodes[6]

// JSON:/teams
const int team1Start = a; // teams[0].start
const int team2Start = c; // teams[1].start
const int team1Cap = 4; // teams[0].capacity
const int team2Cap = 4; // teams[1].capacity

// JSON:/resources initial quantities
const int inita = 2; // resources[1] node a qty 2
const int initb = 0;
const int initc = 4; // resources[0] node c qty 4
const int initd = 2; // resources[2] node d qty 2
const int inite = 0;
const int initf = 0;
const int initg = 0;

// ASSUMPTION: per-node maximum capacity set to total initial resources (8)
const int maxa = 8;
const int maxb = 8;
const int maxc = 8;
const int maxd = 8;
const int maxe = 8;
const int maxf = 8;
const int maxg = 8;

// JSON:/constraints/safety_probs
const double GREEN = 0.99;
const double YELLOW = 0.85;
const double RED = 0.5;

const double factor_team1 = 1.0;
const double factor_team2 = 1.0;

module resources
    // resource counts per node
    xa : [0..maxa] init inita; // JSON:/graph/nodes[0] and JSON:/resources for a
    xb : [0..maxb] init initb;
    xc : [0..maxc] init initc; // JSON:/resources for c
    xd : [0..maxd] init initd; // JSON:/resources for d
    xe : [0..maxe] init inite;
    xf : [0..maxf] init initf;
    xg : [0..maxg] init initg;
endmodule

// TEAM 1 module (teams[0].id = T1)
module team1
    locteam1 : [-1..6] init team1Start; // JSON:/teams[0].start

    // JSON:/graph/edges[0] a->b (R)
    [team1_a_b_0] (locteam1=a)&(xa>=0)&(xb<=maxb-0) -> RED*factor_team1:(xb'=xb+0)&(xa'=xa-0)&(locteam1'=b)+(1-RED*factor_team1):(xa'=xa-0)&(locteam1'=fail);
    [team1_a_b_1] (locteam1=a)&(xa>=1)&(xb<=maxb-1) -> RED*factor_team1:(xb'=xb+1)&(xa'=xa-1)&(locteam1'=b)+(1-RED*factor_team1):(xa'=xa-1)&(locteam1'=fail);
    [team1_a_b_2] (locteam1=a)&(xa>=2)&(xb<=maxb-2) -> RED*factor_team1:(xb'=xb+2)&(xa'=xa-2)&(locteam1'=b)+(1-RED*factor_team1):(xa'=xa-2)&(locteam1'=fail);
    [team1_a_b_3] (locteam1=a)&(xa>=3)&(xb<=maxb-3) -> RED*factor_team1:(xb'=xb+3)&(xa'=xa-3)&(locteam1'=b)+(1-RED*factor_team1):(xa'=xa-3)&(locteam1'=fail);
